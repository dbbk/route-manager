{
  "presets": [
    ["env", {"modules": false}],
    "react"
  ],
  "plugins": [
    "flow-react-proptypes",
    "transform-class-properties",
    "transform-object-rest-spread",
    "styled-components-named",
    "external-helpers",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "src": "./src"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
