[ignore]
<PROJECT_ROOT>/\.storybook/config\.js
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/cypress/.*
<PROJECT_ROOT>/node_modules/config-chain/.*
<PROJECT_ROOT>/node_modules/cypress/.*
<PROJECT_ROOT>/svgs/.*
<PROJECT_ROOT>/commitlint\.config\.js
<PROJECT_ROOT>/jest\.config\.js
<PROJECT_ROOT>/plopfile\.js
<PROJECT_ROOT>/rollup\.config\.js
<PROJECT_ROOT>/setupTests\.js
.*/react-generators/.*
.*/storybook-static/.*

[include]

[libs]

[lints]
all=warn
unclear-type=off
sketchy-null=off

[options]
module.name_mapper='^@festicket\/react-ui-components$' -> '<PROJECT_ROOT>/src/'
module.name_mapper='^src/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
include_warnings=true

[strict]
