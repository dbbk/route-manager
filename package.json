{
  "name": "@festicket/react-ui-components",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "generate": "plop",
    "build": "rollup --config",
    "lint:js": "eslint ./src/**/* --ext .js --ext .jsx",
    "svg": "svgr svgs -d src/components --template svg.template.js --single-quote --no-title",
    "lint": "npm run lint:js && npm run flow",
    "flow": "flow",
    "glow": "glow",
    "flow:libdefs": "flow gen-flow-files src/ --out-dir dist/",
    "flow:typed": "npx flow-typed install --overwrite --ignoreDeps peer",
    "test": "jest",
    "start": "start-storybook -p 9001 -c .storybook",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run flow && lint-staged",
    "prestart": "npm run flow:typed",
    "pretest": "npm run flow:typed",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "build-storybook": "build-storybook",
    "percy-storybook": "percy-storybook"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.4.0",
    "styled-tools": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-angular": "^6.0.2",
    "@festicket/eslint-config": "^1.1.5",
    "@festicket/react-generators": "^0.1.4",
    "@percy-io/percy-storybook": "^1.3.2",
    "@storybook/react": "^3.3.10",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-flow-react-proptypes": "^14.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-jest": "^1.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.3.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.0.2",
    "jest-styled-components": "^4.11.0-0",
    "lint-staged": "^6.0.1",
    "plop": "^1.9.1",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "sinon": "^4.2.2",
    "semantic-release": "^12.4.1",
    "svgr": "^1.7.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/festicket/react-ui-components.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
